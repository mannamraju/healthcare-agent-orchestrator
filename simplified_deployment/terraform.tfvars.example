# Example terraform.tfvars file for Healthcare Agent deployment
# Replace the values with your actual Azure subscription and resource group information

# Required values
subscription_id      = "12345678-1234-1234-1234-123456789012"  # Your Azure subscription ID
resource_group_name  = "healthcare-agent-rg"                   # Your resource group name
environment_name     = "dev"                                    # Environment identifier (dev, test, prod)

# Optional values with defaults
location             = "westus"                                 # Azure region for deployment
openai_model_capacity = 30                                      # Capacity for OpenAI model (TPM/1000)
clinical_notes_source = "blob"                                  # Source for clinical notes (fhir, blob, fabric)
